#: src/lib.c:76
#, c-format
msgid "I/O error reading `%s'"
msgstr "Chyba I/O při čtení `%s'"

#: src/lib.c:78
#, c-format
msgid "Unexpected EOF reading `%s'"
msgstr "Neočekávaný konec souboru při čtení `%s'"

#: src/lib.c:90 src/lib.c:100
msgid "Can not allocate memory"
msgstr "Nemohu alokovat paměť"

#: src/lib.c:131 src/updatedb.c:217
#, c-format
msgid "Can not open `%s'"
msgstr "Nemohu otevřít `%s'"

#: src/lib.c:142
#, c-format
msgid "`%s' does not seem to be a mlocate database"
msgstr "`%s' zřejmě není databáze mlocate"

#: src/lib.c:147
#, c-format
msgid "`%s' has unknown version %u"
msgstr "`%s' má neznámou verzi %u"

#: src/updatedb.c:85
#, c-format
msgid "Too many entries in directory `%s'"
msgstr "Příliš mnoho položek v adresáři `%s'"

#: src/updatedb.c:220
msgid "Can not change directory to `/'"
msgstr "Nemohu změnit adresář na `/'"

#: src/updatedb.c:223
#, c-format
msgid "I/O error while writing to `%s'"
msgstr "Chyba I/O při zápisu do `%s'"

#: src/updatedb.c:225
#, c-format
msgid "I/O error closing `%s'"
msgstr "Chyba I/O při zavírání `%s'"
